{
  "name": "@cassina/langgraphjs-checkpoint-firestore",
  "version": "1.0.0",
  "description": "LangGraph.js Checkpoint implementation for Firestore",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "index.d.cts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cassina/langgraphjs-checkpoint-firestore.git"
  },
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit",
    "emulator": "firebase emulators:start --only firestore",
    "lint": "npx eslint ./src --max-warnings 0",
    "test": "npm run test:build && npm run test:unit && npm run test:int && npm run test:e2e",
    "test:build": "npm run build && jest test/build.test.ts --coverageThreshold '{}' && rm -rf dist",
    "test:e2e": "firebase emulators:exec 'npm run build && jest -c jest.e2e.config.ts test/FirestoreSaver.e2e.test.ts'",
    "test:int": "firebase emulators:exec 'jest test/FirestoreSaver.int.test.ts --runInBand'",
    "test:unit": "jest test/FirestoreSaver.unit.test.ts --runInBand"
  },
  "keywords": ["langgraph", "firestore", "langgraph-checkpoint"],
  "author": "Bernardo Cassina",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^7.11.3"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.2.31 <0.4.0",
    "@langchain/langgraph-checkpoint": "^0.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@langchain/langgraph": "^0.4.1",
    "@langchain/openai": "^0.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^9.32.0",
    "firebase-admin": "^13.4.0",
    "firebase-tools": "^14.11.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
